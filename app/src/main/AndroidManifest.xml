<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="tikhanovskiy.ru.mymoneyflows" >

    <application
        android:name=".application.DBFlowApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.NewCashActivity"
            android:label="@string/title_activity_new_cash" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="tikhanovskiy.ru.mymoneyflows.activity.MainActivity" />
        </activity>
        <activity
            android:name=".activity.NewCategoryActivity"
            android:label="@string/title_activity_new_category" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="tikhanovskiy.ru.mymoneyflows.activity.MainActivity" />
        </activity>
        <activity
            android:name=".activity.NewOperationActivity"
            android:label="@string/title_activity_new_operation" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="tikhanovskiy.ru.mymoneyflows.activity.MainActivity" />
        </activity>
    </application>

</manifest>
